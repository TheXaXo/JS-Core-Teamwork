{{#each brands}}
    <li class="list-group-item">
        <div class="checkbox">
            <label for="checkbox">
                {{name}}
            </label>
        </div>
        <div class="pull-right action-buttons">
            <a href="#/admin/models/add/{{name}}" class="flag"><span class="glyphicon glyphicon-plus"></span></a>
            <a href="#/admin/brands/edit/{{name}}"><span class="glyphicon glyphicon-pencil"></span></a>
            <a href="#/admin/brands/delete/{{_id}}" class="trash"><span class="glyphicon glyphicon-trash"></span></a>
        </div>
    </li>
{{/each}}