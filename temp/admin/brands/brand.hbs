{{#each data}}
    <li class="list-group-item">
        <div class="checkbox">
            <label for="checkbox">
                {{name}}
            </label>
        </div>
        <div class="pull-right action-buttons">

            <a href="#" class="flag"><span class="glyphicon glyphicon-plus">Models</span></a>
            <a href="#/admin/brands/edit/{{name}}"><span class="glyphicon glyphicon-pencil"></span></a>
            <a href="#/admin/brands/delete/{{name}}" class="trash"><span class="glyphicon glyphicon-trash"></span></a>
        </div>
    </li>
{{/each}}