{{#each models}}
    <li class="list-group-item">
        <div class="checkbox">
            <label for="checkbox">
                {{name}}
            </label>
        </div>
        <div class="pull-right action-buttons">
            <a href="#/admin/models/edit/{{_id}}"><span class="glyphicon glyphicon-pencil"></span></a>
            <a href="#/admin/models/delete/{{_id}}" class="trash"><span
                    class="glyphicon glyphicon-trash"></span></a>
        </div>
    </li>
{{/each}}